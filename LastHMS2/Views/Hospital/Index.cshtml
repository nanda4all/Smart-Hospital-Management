@model IEnumerable<LastHMS2.Models.Hospital>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ho_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ho_Subscribtion_Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Active)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ho_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ho_Subscribtion_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Ho_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Ho_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Ho_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
